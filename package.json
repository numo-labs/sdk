{
  "name": "fsearch-sdk",
  "version": "0.1.0",
  "description": "Federated Search SDK. ",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "node_modules/.bin/tape -r babel-register tests/*.js tests/**/*.js | node_modules/.bin/tap-spec",
    "build": "node_modules/.bin/browserify index.js -t [babelify] -s SDK | uglifyjs > dist/sdk.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numo-labs/sdk.git"
  },
  "keywords": [
    "Federated",
    "Search",
    "SDK"
  ],
  "author": "Team Awesome",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/numo-labs/sdk/issues"
  },
  "homepage": "https://github.com/numo-labs/sdk#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {}
}
